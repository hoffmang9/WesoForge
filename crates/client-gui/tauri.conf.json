{
  "productName": "WesoForge",
  "version": "1.0.0",
  "identifier": "com.ealrann.bbr-client",
  "build": {
    "beforeDevCommand": "pnpm -C ../ui dev",
    "beforeBuildCommand": "pnpm -C ../ui build",
    "devUrl": "http://127.0.0.1:5173",
    "frontendDist": "../../ui/dist"
  },
  "app": {
    "windows": [
      {
        "title": "WesoForge",
        "width": 1100,
        "height": 760,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ]
  }
}
